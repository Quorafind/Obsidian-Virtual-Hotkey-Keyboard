{
    "name": "obsidian-virtual-hotkey-keyboard",
    "version": "1.1.3",
    "description": "A plugin for showing hotkeys based on a keyboard layout in Obsidian.",
    "main": "main.js",
    "scripts": {
        "lint": "eslint . --ext .ts",
        "dev": "npm run lint && vite build --watch --mode=development",
        "build:dev": "vite build --watch --mode=development",
        "build:prod": "vite build --mode=production",
        "bumpversion": "node version-bump.mjs && git add manifest.json versions.json",
        "release": "npm run build:prod && npm run bumpversion && git add ."
    },
    "keywords": [],
    "author": "Boninall",
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.1",
        "@tailwindcss/line-clamp": "^0.4.0",
        "@tsconfig/svelte": "^3.0.0",
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.32.0",
        "@typescript-eslint/parser": "5.32.0",
        "autoprefixer": "^10.4.8",
        "builtin-modules": "3.3.0",
        "eslint": "^8.21.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "monkey-around": "^2.3.0",
        "obsidian": "latest",
        "postcss": "^8.4.14",
        "postcss-replace": "^2.0.0",
        "svelte": "^3.49.0",
        "svelte-check": "^2.8.0",
        "svelte-preprocess": "^4.10.7",
        "svelte-select": "5.0.0-beta.16",
        "tailwindcss": "^3.1.7",
        "tslib": "2.4.0",
        "typescript": "4.7.4",
        "vite": "^3.0.0"
    }
}
